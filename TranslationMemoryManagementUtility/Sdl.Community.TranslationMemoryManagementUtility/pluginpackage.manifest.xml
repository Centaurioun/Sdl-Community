  <Author>RWS AppStore Team</Author>
  <Include>
    <File>Sdl.Community.Utilities.TMTool.Lib.dll</File>
    <File>Sdl.Community.Utilities.TMTool.Task.dll</File>
    <File>Sdl.Community.Utilities.TMTool.Tasks.RemapTMX.dll</File>
    <File>Sdl.Community.Utilities.TMTool.Tasks.RemoveDuplicates.dll</File>
    <File>Sdl.Community.Utilities.TMTool.Tasks.RevertIndex.dll</File>
